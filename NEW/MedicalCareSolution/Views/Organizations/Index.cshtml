@model IEnumerable<HealthPlanPortal.Models.Organization>

@{
    ViewBag.Title = "Organization";
}

<h2>Organization</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationPhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationEmailAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationZip)
        </th>
        <th>Actions</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationPhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationEmailAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationCity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrganizationZip)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OrganizationId }) |
            @Html.ActionLink("Details", "Details", new { id=item.OrganizationId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OrganizationId })
        </td>
    </tr>
}

</table>
