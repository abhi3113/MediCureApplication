@model HealthPlanPortal.Models.HealthPlan

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <div>
        <h4>Health Plan Details</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("Code")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.HealthPlanCode)
            </dd>
    
            <dt>
                @Html.DisplayName("Description")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.HealthPlanDescription)
            </dd>
    
            <dt>
                @Html.DisplayName("Required PCP?")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.PCPrequiredBool)
            </dd>
    
            <dt>
                @Html.DisplayName("Is Current PCP In Network?")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.PCPNetworkBool)
            </dd>
    
            <dt>
                @Html.DisplayName("Deductible ID")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.Deductible.DeductibleId)
            </dd>
    
            <dt>
                @Html.DisplayName("Major Medical ID")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.MajorMedical.MajorMedicalId)
            </dd>
    
            <dt>
                @Html.DisplayName("Preventive Care ID")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.PreventiveCare.PreventiveCareId)
            </dd>
    
        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.HealthPlanId }) |
        @Html.ActionLink("Back to List", "Index")
    </p>
</body>
</html>
