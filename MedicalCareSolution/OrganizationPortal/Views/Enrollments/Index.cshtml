    Layout = "~/Views/ManageEmployees.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentTypeCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentTypeDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentTypeCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentTypeDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentType)
